    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "Team Member"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "headshot": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "is_alumni": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "is_lead": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "short_description": {
      "type": "text",
      "required": true
    }
  }
}
